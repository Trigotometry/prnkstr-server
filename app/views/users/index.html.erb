<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name |</th>
      <th>Fill Murray |</th>
      <th>Place Cage |</th>
      <th>Custom Header |</th>
      <th>Custom Header Text |</th>
      <th>Paragraph Background |</th>
      <th>Paragraph Color |</th>
      <th>Snap |</th>
	  <th>Master</th>
      <!-- <th colspan="4"></th> -->
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.fill_murray %></td>
        <td><%= user.place_cage %></td>
        <td><%= user.custom_header %></td>
        <td><%= user.custom_header_text %></td>
        <td><%= user.paragraph_background %></td>
        <td><%= user.paragraph_color %></td>
        <td><%= user.snap %></td>
		<td><%= user.master_id %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %> |
<%= link_to 'Masters', masters_path %>
